<script type="text/javascript">
function Logout() {
	var formObj = document.getElementById("Form1");
	if (formObj == null) {
	    formObj = document.getElementById("aspnetForm");
	}
	SSOLogoutForm(formObj);
}
function goLogOut() {
    var Frm = document.getElementById("Form1");
	try {
		if(GetCookie('ssl') == "1") {
			document.getElementById('tSecureConnect').value = "1";
		}else {
			document.getElementById('tSecureConnect').value = "0";
		}
	} catch(e) {	
		Frm.action = 'http://secure.altools.co.kr/AltoolsSSO/Ticket/SSOTicket.aspx';
	}

	ALToolsSSO_SiteDomain		= document.getElementById('tSiteDomain').value;
	ALToolsSSO_ReturnUrl		= document.getElementById('tSurl').value;
	ALToolsSSO_SecureConnect	= document.getElementById('tSecureConnect').value;
	
	document.getElementById('tAuthMode').value = 'logout';
	if(document.getElementById('tSecureConnect').value == "1") {
		Frm.action= 'https://secure.altools.co.kr/AltoolsSSO/Ticket/SSOTicket.aspx';
	} else {
		Frm.action= 'http://secure.altools.co.kr/AltoolsSSO/Ticket/SSOTicket.aspx';
	}
	Frm.__VIEWSTATE.name="NOVIEWSTATE";
	Frm.submit();	
}
</script>
