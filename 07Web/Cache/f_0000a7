<script>
    $(document).ready(function () {

        var chash = window.location.hash;
        if (chash && chash !== '#n') {
            showSubDescription(chash);
        }

        $('#showLayerEgg').click(function (e) {
            var divTop = $('#showLayerEgg').position().top - $('#eggSolidLayer').outerHeight() + 100;
            var divLeft = $('#showLayerEgg').position().left + $('#eggSolidLayer').outerWidth() - 200;

            $('#eggSolidLayer').css({
                "top": divTop,
                "left": divLeft,
                "position": "absolute"
            }).show();
        });
    });

    function CloseLayerEgg(id) {
        $('#' + id).hide();
    }   

</script>

