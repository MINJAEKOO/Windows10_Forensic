<script type="text/javascript" src="/JS/jquery.visible.min.js"></script>

<script type="text/javascript">
    $(document).ready(function () {

        var chash = window.location.hash;

        if (getParam('page') != '') {
            // 히스토리 보기 관련 기능
            $(".main_content > h3").removeClass("on");
            $(".main_content > div").hide();

            $(".main_content > h3").eq(2).addClass("on");
            $(".main_content > div").eq(2).show();
        }
        else if (!chash || chash == '#n') {

            $(".main_content > h3").eq(0).addClass("on");
            $(".main_content > div").eq(0).show();
        }
        else if (chash && chash !== '#n') {
            if (chash == '#lyrics_site') {
                showSubDescription('#livelyrics');
            }
            else if (chash == '#ocr_text') {
                //showSubDescription('#livelyrics');
            }
            else {
                showSubDescription(chash);
            }
        }

        // 주요기능 항목(.main_function)의 첫번째 엘리먼트의 전체 내용이 보이는 경우 또는 스크롤이 50%가 내려온 경우 GA로 이벤트 전송
        // library : https://github.com/customd/jquery-visible
        var timer;

        $(window).scroll(function () {
            if (timer) {
                window.clearTimeout(timer);
            }

            timer = window.setTimeout(function () {
                var $mainFunction = $(".main_function:visible");
                var scrollTop = $(window).scrollTop();
                var docHeight = $(document).height();
                var winHeight = $(window).height();
                var scrollPercent = (scrollTop) / (docHeight - winHeight);
                var scrollPercentRounded = Math.round(scrollPercent * 100);
                if ($mainFunction.children().first().visible() || scrollPercentRounded >= 50) {
                    var eventCategory = $mainFunction.attr("data-event-category");
                    var eventAction = $mainFunction.attr("data-event-action");
                    var eventLabel = $mainFunction.attr("data-event-label");
                    sendOneEventToGA(eventCategory, eventAction, eventLabel);
                }
            }, 500);
        });
    });
</script>
</html>
