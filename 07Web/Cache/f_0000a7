    function ALToolsDownload(name, version, url, type, isBlank) {
        if ((url.indexOf("www.altools.jp") > -1) || (url.indexOf("www.altools.com") > -1) || (url.indexOf("www.almap.co.kr") > -1) || (url.indexOf("alyac.altools.co.kr") > -1)) {
            window.open(url, "_blank");
        }
        else {
            CheckALDown(name, version);
            if (isBlank) {
                window.open(url, "_blank");
            } else {
                fileDownV(url, type);
            }
        }
    }

    function showSubDescription(descName) {

        $(".tab_content").hide();
        $(".main_content > h3").removeClass("on");
        $(".main_content > div").hide();

        $(descName).parent().find('img').hide();
        $(descName).find('img').show();
        $(descName).siblings('div').find('img').hide();

        $(descName).find('img').each(function () {
            $(this).css('visibility', 'visible');
            $(this).show();
        });

        $(descName).siblings('div').find('img').each(function () {
            $(this).css('visibility', 'hidden');
            $(this).hide();
        });

        $(descName).siblings('div').css('visibility', 'hidden');
        $(descName).siblings('div').hide();
        $(descName).css('visibility', 'visible');
        $(descName).show();

        $(descName).parent().prev().addClass("on");
        $(descName).parent().show();
        $(descName).parent().children('ul.tab_snb').children('li').children('a').removeClass('on');
        $(descName).parent().children('ul.tab_snb').children('li').children('a[href=' + descName + ']').addClass('on');

        scrollTo(0, $(descName).parent().prev().offset().top);
    }    
    </script>	  
    
