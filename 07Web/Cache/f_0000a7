    function snsBtnClick(snsType) {                
        switch(snsType)
        {
            case 'Twitter':
                window.open('http://twitter.com/intent/tweet?text=%5B%EC%95%8C%ED%88%B4%EC%A6%88%5D%20%EC%95%8C%EC%A7%91%31%30%2E%38%38&url=http%3a%2f%2fgoo.gl%2ft3U4Pw&hashtags=ALTOOLS');                
                break;
            case 'FaceBook':
                window.open('http://www.facebook.com/sharer.php?u=https%3a%2f%2fwww.altools.co.kr%2fdownload%2falzip.aspx');
                break;
        }                          
        return false;
    }
    
</script>

